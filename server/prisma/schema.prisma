// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  sid    String   @id
  sess   Json
  expire DateTime

  @@map(name: "session")
}

model User {
  id       String  @id
  email    String  @unique
  name     String?
  username String? @unique

  @@map(name: "users")
}
